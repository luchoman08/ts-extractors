{
    "name": "ts-extract-abstract",
    "displayName": "Ts extractors",
    "description": "Automatically generates an abstract class based on a class or interface",
    "version": "0.1.1",
    "publisher": "ismael",
    "repository": {
        "type": "git",
        "url": "https://github.com/luchoman08/ts-extractors.git"
    },
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.extract"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.extract",
                "title": "Typescript extractors"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "publish": "vsce publish",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42",
        "vsce": "^1.42.0"
    },
    "dependencies": {
        "typescript-parser": "^2.4.0"
    }
}
